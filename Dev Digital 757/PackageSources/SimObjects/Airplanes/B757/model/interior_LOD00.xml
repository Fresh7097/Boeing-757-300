<Animation name="lever_landing_gear" guid="6176c7db-05b8-47d7-b97c-27febe6f2106" length="100" type="Sim" typeParam2="lever_landing_gear" typeParam="AutoPlay" />
    <PartInfo>
        <Name>lever_landing_gear</Name>
        <AnimLength>100</AnimLength>
        <Animation>
            <Parameter>
                <Sim>
                    <Variable>GEAR HANDLE POSITION</Variable>
                    <Units>percent</Units>
                </Sim>
                <Lag>200</Lag>
            </Parameter>
        </Animation>
        <MouseRect>
            <Cursor>Hand</Cursor>
            <HelpID>HELPID_GAUGE_GEAR_LEVER</HelpID>
            <TooltipID>TOOLTIPTEXT_GEAR_LEVER</TooltipID>
            <MouseFlags>LeftSingle+LeftDrag+Wheel</MouseFlags>
            <CallbackJumpDragging>
                <YMovement>
                    <Delta>50</Delta>
                    <EventIdInc>GEAR_DOWN</EventIdInc>
                    <EventIdDec>GEAR_UP</EventIdDec>
                </YMovement>
            </CallbackJumpDragging>
        </MouseRect>
    </PartInfo>



